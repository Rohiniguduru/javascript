// Define an object with a property and a setter method
const person = {
    firstName: '',
    lastName: '',
    
    // Define a setter method for the fullName property
    set fullName(fullName) {
      const names = fullName.split(' ');
      this.firstName = names[0];
      this.lastName = names[1];
    }
  };
  
  // Set the fullName property using the setter
  person.fullName = 'rohini guduru';
  
  // Access the individual properties
  console.log(person.firstName); // Output: rohini
  console.log(person.lastName);  // Output: guduru
